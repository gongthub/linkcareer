
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <meta charset="utf-8" name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=0.5, maximum-scale=2.0, user-scalable=yes" />
    <title>中国青少年发展服务中心</title>
    <link href="~/Content/css/main.css?v=2" rel="stylesheet" />

</head>
<body>
    <div class="dvCent">
        <div class="dvCentTopimg">
            <img src="~/Content/images/login.png" />
        </div>
        <div class="dvCentTopinput">
            <form name="form1" action="/Main/Search" method="post">
                <div style="margin-top:1vw;" class="dvCentTopinput_one">
                    <input type="text" placeholder="  姓名：" name="name" />
                </div>
                <div>
                    <input type="text" placeholder="  身份证号：" name="IdNumber" />
                </div>
                <div>
                    <input type="text" placeholder="  证书编号：" name="CertificateNumber" />
                </div>
                <div>
                    <input class="dvCentbtn" type="button" value="查询" onclick=" document.form1.submit();" /><span class="dvCentdesc">提示：任意输入以上两项即可查询</span>
                </div>
            </form>
        </div>
    </div>
</body>
</html>
